{
  "name": "treehouselabs/queue-bundle",
  "type": "symfony-bundle",
  "description": "Symfony2 bundle for message queue",
  "keywords": ["mq","message queue","message","queue","amqp","rabbitmq","worker","pubsub"],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Kruithof",
      "email": "peter@treehouse.nl"
    }
  ],
  "minimum-stability": "beta",
  "require": {
    "php": ">=7.1",
    "doctrine/orm": "^2.3",
    "psr/log": "^1.0",
    "symfony/config": "^4.0|^5.0",
    "symfony/console": "^4.0|^5.0",
    "symfony/dependency-injection": "^4.0|^5.0",
    "symfony/framework-bundle": "^4.0|^5.0",
    "symfony/http-kernel": "^4.0|^5.0",
    "treehouselabs/queue": "^0.5"
  },
  "require-dev": {
    "ext-amqp": "^1.7",
    "matthiasnoback/symfony-dependency-injection-test": "^3.1|^4.1",
    "phpunit/phpunit": "^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\QueueBundle\\": "src/TreeHouse/QueueBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TreeHouse\\QueueBundle\\Tests\\": "tests/src/TreeHouse/QueueBundle/Tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
